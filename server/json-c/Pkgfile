version=0.12
source=(
https://s3.amazonaws.com/${name}_releases/releases/$name-$version.tar.gz)

build(){
    cd $name-$version
    CFLAGS="$CFLAGS -Wno-error"
    ./configure --prefix=/usr \
                --disable-static
    make check
    make DESTDIR=$PKG install    
}



# NuTyX Pkgfile (http://nutyx.org)
